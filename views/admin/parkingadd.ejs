<body>
    <%-include('../../partials/admin/header.ejs');%>
<section id="mid">
    <div class="container">
        <div class="row">
            
            <%-include('../../partials/admin/left.ejs');%>
            <div class="col-md-9">
            <h2 class="text-center text-primary">Add New Entry Here</h2>
            <form action="/admin/parkingrecord" method="post">
                <label for="" class="form-label mt-2">Vechile No</label>
                <input type="text" name="vecno" class="form-control">
                <label for="" class="form-label">Vechile Type</label>
                <select name="vtype" id="" class="form-select">
                    <option value="2w">2 Wheeler</option>
                    <option value="3w">3 Wheeler</option>
                    <option value="4w">4 Wheeler</option>
                <option value="lw">Light Vechile</option>
                <option value="hw">Heavy Vechile</option>
                </select>
                <label for="" class="form-label">Vechile Time</label>
                <input type="text" name="vintime" class="form-control">
                <button type="submit" class="btn btn-success mt-2 mb-4">Add</button>

            </form>
            </div>
        </div>
    </div>
</section>
<%-include('../../partials/admin/footer.ejs');%>


    
